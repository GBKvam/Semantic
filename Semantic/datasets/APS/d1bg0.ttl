# D1 limited trade area

@prefix : <https://www.sdir.no/SDIR_Simulator/shapes#> .
@prefix sdir: <https://www.sdir.no/SDIR_Simulator#> .
@prefix cert: <https://www.sdir.no/SDIR_Simulator/shapes/certificate#> .
@prefix course: <https://www.sdir.no/SDIR_Simulator/shapes/course#> .
@prefix sgs: <https://www.sdir.no/SDIR_Simulator/shapes/sgs#> .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix unit: <http://qudt.org/vocab/unit/> .

:NS_D1BG0
    a sh:NodeShape ;
    sh:targetClass sdir:D1BG0 ;
    sh:description "Shape describing requirements for D1BG0 certificate."@en ,
                   "Shape som beskriver krav knyttet til sertifikatet D1BG0."@no ;
    sh:property :PS_Age_minIncl_20, course:AdvancedSafetyCourseDeckOfficerLimited, :PS_VD2_Education ;
    sh:and (cert:PS_GMDSSRadioCertificate cert:PS_HealthCertificate) ;
    sh:or (
        [ sh:and (
            [ sh:or (cert:PS_D2A0 cert:PS_D2B0 cert:PS_D2BG0 cert:PS_D3BG0 cert:PS_D3BG30 cert:PS_D3A0 cert:PS_D4B0 cert:PS_D4D0 cert:PS_D4F0) ]

            [ sh:path sdir:hasSeagoingServiceRequirement ;
              sh:hasValue sdir:SGS_500_L_1_1080_M_V_DO ;
              sh:order 1 ; ]
        )]
        [ sh:and (
            [ sh:path sdir:hasSeagoingServiceRequirement ;
              sh:hasValue sdir:SGS_500_L_1_360_M_V_CO ;
              sh:order 2 ; ]

            [ sh:path sdir:hasSeagoingServiceRequirement ;
              sh:hasValue sdir:SGS_500_L_1_720_M_V_DO ;
              sh:order 2 ; ]
        )]
    ) ;
    sh:property [
        sh:path sdir:hasSeagoingServiceRequirement ;
        sh:in (sdir:SGS_500_L_1_1080_M_V_DO sdir:SGS_500_L_1_360_M_V_CO sdir:SGS_500_L_1_720_M_V_DO) ;
    ] .

:PS_Age_minIncl_20
    a sh:PropertyShape ;
    sh:description "Value of age must be at least 20."@en ;
    sh:description "Verdien av age må være minst 20."@no ;
    sh:path sdir:age ;
    sh:minInclusive 20 ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:datatype unit:YR .

:PS_VD2_Education
    a sh:PropertyShape ;
    sh:description "Requires VD2 education."@en ;
    sh:description "Krever VD2-utdanning."@no ;
    sh:path sdir:requiresEducation ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:hasValue sdir:VD2 .